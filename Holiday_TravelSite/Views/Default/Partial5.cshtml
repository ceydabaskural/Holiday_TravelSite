@using Holiday_TravelSite.Models.Entities
@model List<Blog>

<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.ImageUrl" alt="" style="120px" height="65px" />
        </div>
        <div class="bot-desc1">
            <p>@item.Description.Substring(0, 80)...</p>
        </div>
        <div class="clear"></div>
    }
</div>

