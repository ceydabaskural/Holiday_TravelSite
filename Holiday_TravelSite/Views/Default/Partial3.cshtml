@using Holiday_TravelSite.Models.Entities
@model List<Blog>
<div class="padding">
    <h4>This Week's Top 10</h4>
    <ul class="list list-pad">
        @{ int count = 0;}
        @foreach (var item in Model)
        {
            count++;
            <li class="list-bg"><span>@count.</span><a href="/Blog/Detail/@item.BlogId">@item.Title</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">See all blogs</a>
</div>
